{
	"name": "__MSG_appName__",
	"short_name": "BetterTDeck",
	"default_locale":"en",
	"version": "1.4.6",
	"manifest_version": 2,
	"description": "__MSG_appDesc__",
	"content_scripts": [{
		"matches": ["https://tweetdeck.twitter.com/*","http://tweetdeck.twitter.com/*"],
		"js": ["js/injectScript.js","js/content.js","js/emojiToImage.js","js/usefulFunctions.js"],
		"css": ["content.css","emojis.css"]
		}],
	"icons": { "16": "icon16.png",
	           "48": "icon48.png",
	          "128": "icon128.png" },
	"permissions": ["storage"],
	"options_page": "fancy-settings/source/index.html",
	"background" : { "page": "background.html" },
	"permissions": ["<all_urls>"],
	"web_accessible_resources": ["font/*","emojis-s8bc023ba47.png","emojis2x-s1940453998.png","js/emoji.json","emoji-pngs/emojis-inline/*","emoji-popover.html"]
}
