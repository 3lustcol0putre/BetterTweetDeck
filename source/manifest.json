{
	"name": "Better TweetDeck",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Improves the experience on web version of TweetDeck",
	"content_scripts": [{
		"matches": ["https://tweetdeck.twitter.com/*","http://tweetdeck.twitter.com/*"],
		"js": ["content.js"],
		"css": ["wbs.css"]
		}],
	//"web_accessible_resources": ["bg-hover-top.png","wbs-.svg"],
	"icons": { "16": "icon16.png",
	           "48": "icon48.png",
	          "128": "icon128.png" },
	"permissions": ["<all_urls>"]
}
