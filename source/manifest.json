{
	"name": "Better TweetDeck",
	"short_name": "BetterTDeck",
	"default_locale":"en",
	"version": "1.3.3",
	"manifest_version": 2,
	"description": "__MSG_appDesc__",
	"content_scripts": [{
		"matches": ["https://tweetdeck.twitter.com/*","http://tweetdeck.twitter.com/*"],
		"js": ["jquery.ajax.min.js","injectScript.js","content.js","emojiToImage.js"],
		"css": ["content.css"]
		}],
	"icons": { "16": "icon16.png",
	           "48": "icon48.png",
	          "128": "icon128.png" },
	"permissions": ["storage"],
	"options_page": "fancy-settings/source/index.html",
	"background" : { "page": "background.html" },
	"permissions": ["<all_urls>"]
}
