{
	"name": "__MSG_appName__",
	"short_name": "BetterTDeck",
	"default_locale":"en",
	"version": "1.5.3",
	"manifest_version": 2,
	"description": "__MSG_appDesc__",
	"content_scripts": [{
		"matches": ["https://tweetdeck.twitter.com/*","http://tweetdeck.twitter.com/*"],
		"js": ["js/injectScript.js","js/content.js","js/emojiToImage.js","js/usefulFunctions.js","js/konami-js/konami.js"],
		"css": ["content.css","emojis.css"]
		}],
	"icons": { "16": "icon16.png",
	           "48": "icon48.png",
	          "128": "icon128.png" },
	"permissions": ["storage"],
	"options_page": "fancy-settings/source/index.html",
	"background" : { "page": "background.html" },
	"permissions": [
		"https://images4-focus-opensocial.googleusercontent.com/*",
		"https://noembed.com/*",
		"http://api.imgur.com/*",
		"http://cl.ly/*",
		"http://d.pr/*",
		"http://api.instagram.com/*",
		"http://instagr.am/p/*",
		"https://www.flickr.com/services/oembed/*",
		"https://api.500px.com/*",
		"https://*.media.tumblr.com/*",
		"http://vimeo.com/api/*",
		"https://api.dailymotion.com/*",
		"http://backend.deviantart.com/*",
		"http://img.ly/show/*",
		"http://yfrog.com/*",
		"http://moby.to/*",
		"http://soundcloud.com/*",
		"http://api.embed.ly/1/*",
		"http://erambert.me/others/konamibtd/*",
		"https://tweetdeck.twitter.com/*",
		"tabs",
		"contextMenus"
	],
	"web_accessible_resources": ["font/*","emojis-s8bc023ba47.png","emojis2x-s1940453998.png","emoji-pngs/emojis-inline/*","emoji-popover.html","fancy-settings/source/index.html"]
}
