{
  "name": "bettertweetdeck",
  "version": "4.0.0",
  "description": "Adds some nice options on TweetDeck to provide a better experience on the webapp when used on Chrome.",
  "repository": {
    "type": "git",
    "url": "https://github.com/eramdam/BetterTweetDeck.git"
  },
  "author": "Damien Erambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eramdam/BetterTweetDeck/issues"
  },
  "scripts": {
    "_start": "cross-env NODE_ENV=development webpack --watch --progress --env",
    "start:firefox": "npm run _start -- browser=firefox",
    "start:chrome": "npm run _start -- browser=chrome",
    "serve": "webpack serve --env",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --env",
    "build:prod": "cross-env NODE_ENV=production webpack --hide-modules --env",
    "fix": "eslint --fix . --ext .jsx,.js,.ts,.tsx",
    "lint": "eslint . --ext .jsx,.js,.ts,.tsx",
    "typecheck:watch": "tsc -p . --watch",
    "typecheck": "tsc -p .",
    "postinstall": "node-config-ts",
    "run:firefox": "web-ext run -s dist",
    "test": "npm run lint && npm run typecheck",
    "release": "npm run build:prod -- browser=chrome && npm run pack:chrome && npm run build:prod -- browser=firefox && npm run pack:firefox",
    "pack:chrome": "cross-env NODE_ENV=development node tools/pack.js",
    "pack:firefox": "cross-env NODE_ENV=development web-ext build -s dist -a artifacts --overwrite-dest"
  },
  "browserslist": [
    "last 1 chrome versions",
    "last 1 firefox versions",
    "last 1 edge versions",
    "last 1 safari versions"
  ],
  "homepage": "https://github.com/eramdam/BetterTweetDeck",
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@types/emoji-mart": "^3.0.2",
    "@types/feather-icons": "^4.7.0",
    "@types/file-saver": "^2.0.1",
    "@types/hogan.js": "^3.0.0",
    "@types/jquery": "^3.5.3",
    "@types/lodash": "^4.14.162",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-frame-component": "^4.1.1",
    "@types/react-portal": "^4.0.2",
    "@types/twit": "^2.2.28",
    "@types/webpack": "^4.41.23",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "config": "^3.3.2",
    "cross-env": "^7.0.2",
    "crx": "^5.0.1",
    "css-loader": "^5.0.0",
    "dom-chef": "^4.2.2",
    "emoji-mart": "^3.0.0",
