{
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "movets.skipWarning": true,
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.multiLineTrailingComma": true,
  "typescriptHero.imports.insertSpaceBeforeAndAfterImportBraces": false,
  "typescriptHero.imports.multiLineWrapThreshold": 100,
  "xmlTools.splitXmlnsOnFormat": false,
  "files.eol": "\n",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    },
    "html"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".cache-loader": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "markdown": "javascriptreact"
  },
  "emmet.excludeLanguages": [],
  "typescript.preferences.renameShorthandProperties": false,
  "beautify.ignore": ["*"]
}